{
  "name": "grounding-ai-map",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "make-tiles": "rimraf public/map/map.dzi public/map/map_files && vips dzsave resources/map.jpg public/map/map",
    "make-thumbnail": "rimraf public/map.jpg && vips thumbnail resources/map.jpg public/map.jpg 400",
    "make-summaries": "rimraf public/summaries && tsx bin/build-json-summaries.ts",
    "make-map-dimensions": "rimraf src/map-dimensions.json && echo \"{ \\\"width\\\": $(vips im_header_int width resources/map.jpg), \\\"height\\\": $(vips im_header_int height resources/map.jpg) }\" > src/map-dimensions.json",
    "prepare": "npm run make-tiles && npm run make-thumbnail && npm run make-map-dimensions && npm run make-summaries"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "minisearch": "^7.1.1",
    "openseadragon": "^5.0.1",
    "papaparse": "^5.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-router-typesafe": "^1.5.0",
    "react-select": "^5.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/lodash": "^4.17.14",
    "@types/openseadragon": "^3.0.10",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
